CELERYD_NODES="{{ celeryd_nodes }}"

CELERY_BIN="{{ directory_venv }}/bin/celery"

CELERY_APP="download.tasks"

CELERYD_MULTI="multi"

CELERYD_OPTS="--autoscale={{ celeryd_max_workers }},{{ celeryd_min_workers }}"

CELERYD_PID_FILE="{{ directory_run }}/{{ instance_name }}-generator/download-generator-%n.pid"
CELERYD_LOG_FILE="{{ directory_log }}/download-generator-%n%I.log"
CELERYD_LOG_LEVEL="{{ celeryd_log_level }}"

FLASK_APP=download
FLASK_ENV={{ application_environment }}

DOWNLOAD_SERVICE_SETTINGS={{ application_service_settings }}
